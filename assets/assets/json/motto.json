[
  { "index": 1, "label": "無懼世事\n\t變改" },
  { "index": 2, "label": "祝你在亂流下\n\t平安" },
  { "index": 3, "label": "悠悠乎與\n\t灝氣俱" },
  { "index": 4, "label": "滿招損\n\t謙受益" },
  { "index": 5, "label": "老驥伏櫪\n\t志在千里" },
  { "index": 6, "label": "踏破鐵鞋無覓處\n\t得來全不費工夫" },
  { "index": 7, "label": "欲窮千里目\n\t更上一層樓" },
  { "index": 8, "label": "藍藍天空\n\t高掛我的夢想" },
  { "index": 9, "label": "寧化飛灰\n\t不作浮塵" },
  { "index": 10, "label": "千磨萬擊\n\t還堅韌" },
  { "index": 11, "label": "漫瀾\n\t星河" },
  { "index": 12, "label": "風中\n\t旅人" },
  { "index": 13, "label": "我思\n\t飄飄" },
  { "index": 14, "label": "鍥而不捨\n\t金石可鏤" },
  { "index": 15, "label": "願你出走半生\n\t歸來仍是少年" },
  { "index": 16, "label": "每口呼吸\n\t竟是如此清新" },
  { "index": 17, "label": "純正\n\t快樂" },
  { "index": 18, "label": "銀河\n\t沙漏" },
  { "index": 19, "label": "青春年華\n\t百代留芳" },
  { "index": 20, "label": "銀河星辰\n\t也不及您好" },
  { "index": 21, "label": "死有重於泰山\n\t或輕於鴻毛" },
  { "index": 22, "label": "有志者\n\t事竟成" },
  { "index": 23, "label": "千里之行\n\t始於足下" },
  { "index": 24, "label": "羽之神勇\n\t千古無雙" },
  { "index": 25, "label": "寧為玉碎\n\t不為瓦全" },
  { "index": 26, "label": "燕雀安知鴻鵠之志哉" },
  { "index": 27, "label": "暴霜露\n\t斬荊棘" },
  { "index": 28, "label": "究天人之際\n\t通古今之變" },
  { "index": 29, "label": "風水\n\t輪流轉" },
  { "index": 30, "label": "智者千慮\n\t必有一失" },
  { "index": 31, "label": "愚者千慮\n\t必有一得" },
  { "index": 32, "label": "飛鳥盡\n\t良弓藏" },
  { "index": 33, "label": "狡兔死\n\t走狗烹" },
  { "index": 34, "label": "運籌策帷帳之中\n\t決勝於千里之外" },
  { "index": 35, "label": "雖萬被戮\n\t豈有悔哉" },
  { "index": 36, "label": "兵者\n\t詭道也" },
  { "index": 37, "label": "一夫當關\n\t萬夫莫開" },
  { "index": 38, "label": "王侯將相\n\t寧有種乎" },
  { "index": 39, "label": "奮六世之餘烈\n\t振長策而御宇內" },
  { "index": 40, "label": "天下熙熙\n\t皆為利來" },
  { "index": 41, "label": "當斷不斷\n\t反受其亂" },
  { "index": 42, "label": "待到秋來九月八\n\t我花開後百花殺" },
  { "index": 43, "label": "沖天香陣透長安\n\t滿城盡帶黃金甲" },
  { "index": 44, "label": "人生得意須盡歡\n\t莫使金樽空對月" },
  { "index": 45, "label": "春花秋月\n\t何時了" },
  { "index": 46, "label": "對酒當歌\n\t人生幾何" },
  { "index": 47, "label": "譬如朝露\n\t去日苦多" },
  { "index": 48, "label": "螳螂捕蟬\n\t黃雀在後" },
  { "index": 49, "label": "仁義不施\n\t而攻守之勢異也" },
  { "index": 50, "label": "人棄我取\n\t人取我與" },
  { "index": 51, "label": "人方為刀俎\n\t我為魚肉" },
  { "index": 52, "label": "富貴多士\n\t貧賤寡友" },
  { "index": 53, "label": "夫哀莫大於心死" },
  { "index": 54, "label": "一騎絕塵" }
]
