{
    "not_empty": {
        "regex": ".+",
        "success": "correct not empty input format",
        "fail": "wrong not empty format"
    },
    "email": {
        "regex": "^[\\w-.]+@(?:[\\w-]+\\.)+[a-zA-Z]{2,}$",
        "success": "correct email input format",
        "fail": "wrong email format"
    },
    "url": {
        "regex": "^(https?://)?[\\w.-]+\\.[a-zA-Z]{2,}(?:/\\S*)?$",
        "success": "correct url input format",
        "fail": "wrong url format"
    },
    "password": {
        "regex": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)[a-zA-Z\\d]{8,}$",
        "success": "correct password input format",
        "fail": "wrong password format"
    },
    "number": {
        "regex": "^\\d+$",
        "success": "correct number input format",
        "fail": "wrong number format"
    },
    "date": {
        "regex": "^(0[1-9]|1[0-2])/(0[1-9]|1\\d|2\\d|3[01])/(19|20)\\d{2}$",
        "success": "correct date input format",
        "fail": "wrong date format"
    },
    "ip": {
        "regex": "^(\\d{1,3}\\.){3}\\d{1,3}$",
        "success": "correct ip input format",
        "fail": "wrong ip format"
    }
